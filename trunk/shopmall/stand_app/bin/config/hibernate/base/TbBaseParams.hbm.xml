<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eh.base.entity.TbBaseParams"
		table="TB_BASE_PARAMS">
		<id name="keyId" type="java.lang.Long">
			<column name="KEY_ID" precision="10" scale="0" />
			<generator class="native" />
		</id>
		<property name="keyCode" type="java.lang.String">
			<column name="KEY_CODE" length="10" />
		</property>
		<property name="keyName" type="java.lang.String">
			<column name="KEY_NAME" length="200" />
		</property>
		<many-to-one name="parent"
			class="com.eh.base.entity.TbBaseParams">
			<column name="FATHER_ID" precision="10" scale="0" />
		</many-to-one>
		<property name="isSystem" type="java.lang.Long">
			<column name="IS_SYSTEM" precision="10" scale="0" />
		</property>
		<property name="keyStatus" type="java.lang.Long">
			<column name="KEY_STATUS" precision="10" scale="0" />
		</property>
		<property name="orderNum" type="java.lang.Long">
			<column name="ORDER_NUM" precision="10" scale="0" />
		</property>
		<property name="treeNo" type="java.lang.String">
			<column name="TREE_NO" length="200" />
		</property>
		<property name="createUser" type="java.lang.Long">
			<column name="CREATE_USER" precision="10" scale="0" />
		</property>
		<property name="createDate" type="java.util.Date">
			<column name="CREATE_DATE" length="7" />
		</property>
		<property name="updateUser" type="java.lang.Long">
			<column name="UPDATE_USER" precision="10" scale="0" />
		</property>
		<property name="updateDate" type="java.util.Date">
			<column name="UPDATE_DATE" length="7" />
		</property>
	</class>
</hibernate-mapping>