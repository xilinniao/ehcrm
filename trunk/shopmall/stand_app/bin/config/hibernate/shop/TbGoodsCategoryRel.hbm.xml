<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eh.shop.entity.TbGoodsCategoryRel" table="TB_GOODS_CATEGORY_REL">
		<id name="recId" type="java.lang.Long">
			<column name="REC_ID" precision="10" scale="0" />
			<generator class="native" />
		</id>
		<property name="categoryId" type="java.lang.Long">
			<column name="CATEGORY_ID" precision="10" scale="0" />
		</property>
		<property name="goodsId" type="java.lang.Long">
			<column name="GOODS_ID" precision="10" scale="0" />
		</property>
		<property name="orderNum" type="java.lang.Long">
			<column name="ORDER_NUM" precision="10" scale="0" />
		</property>
	</class>
</hibernate-mapping>