<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.eh.shop.entity.TbArticleCategory" table="TB_ARTICLE_CATEGORY">
		 <id name="categoryId" type="java.lang.Long">
            <column name="CATEGORY_ID" precision="10" scale="0"/>
            <generator class="native" />
      	 </id>
		 <property name="categoryName" type="java.lang.String">
            <column name="CATEGORY_NAME" length="200"/>
        </property>
		 <property name="metaKeyWord" type="java.lang.String">
            <column name="META_KEY_WORD" length="300"/>
        </property>
		 <property name="metaDesc" type="java.lang.String">
            <column name="META_DESC" length="2000"/>
        </property>
		 <property name="orderNum" type="java.lang.Long">
            <column name="ORDER_NUM" precision="10" scale="0"/>
        </property>
    </class>
</hibernate-mapping>