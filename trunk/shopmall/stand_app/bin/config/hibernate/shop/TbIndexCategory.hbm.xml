<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.eh.shop.entity.TbIndexCategory" table="TB_INDEX_CATEGORY">    
		 <id name="categoryId" type="java.lang.Long">
            <column name="CATEGORY_ID" precision="10" scale="0"/>
            <generator class="native" />
      	 </id>
		 <many-to-one name="shopInfo"
			class="com.eh.shop.entity.TbShopInfo">
			<column name="SHOP_ID" precision="12" scale="0" />
		</many-to-one>
		 <property name="categoryName" type="java.lang.String">
            <column name="CATEGORY_NAME" length="50"/>
        </property>
		 <property name="goodsCnt" type="java.lang.Long">
            <column name="GOODS_CNT" precision="10" scale="0"/>
        </property>
		 <property name="orderNum" type="java.lang.Long">
            <column name="ORDER_NUM" precision="10" scale="0"/>
        </property>
		 <property name="showNum" type="java.lang.Long">
            <column name="SHOW_NUM" precision="10" scale="0"/>
        </property>
		 <property name="isShow" type="java.lang.Long">
            <column name="IS_SHOW" precision="10" scale="0"/>
        </property>
    </class>
</hibernate-mapping>