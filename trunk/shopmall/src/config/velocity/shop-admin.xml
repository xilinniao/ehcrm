<?xml version="1.0" encoding="UTF-8"?>
<root>
	<velocity name="shop-admin-goods-info-list">
		<![CDATA[
		{
			"sEcho":"$sEcho",
			"iTotalRecords": "$page.totalCount",
		    "iTotalDisplayRecords": "$page.totalCount",
		    "aaData": [
		    	#foreach( $x in $page.result )
		    		["$x.goodsId",
		    		"$x.goodsName",
		    		"$x.leavesCount"
		    		]#if( $foreach.hasNext ),#end
		    		
		    	#end
		    ]
		}
		]]>
	</velocity>
</root>