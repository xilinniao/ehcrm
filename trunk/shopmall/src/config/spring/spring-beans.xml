<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="loginLogic" parent="TransactionProxyFactoryBean">
		<property name="target">
			<bean class="com.eh.base.common.logic.impl.LoginLogicImpl">
				<property name="baseDao">
					<ref bean="baseDao" />
				</property>
				<property name="baseJdbc">
					<ref bean="baseJdbc" />
				</property>
			</bean>
		</property>
	</bean>
</beans>