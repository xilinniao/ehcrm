<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eh.base.entity.TbParamKind" table="TB_PARAM_KIND">

		<id name="kindCode" type="java.lang.String">
			<column name="kind_code" length="30" />
		</id>

		<property name="kindName" type="java.lang.String">
			<column name="kind_name" length="30" />
		</property>

		<property name="kindDesc" type="java.lang.String">
			<column name="kind_desc" length="100" />
		</property>
		
	</class>
</hibernate-mapping>