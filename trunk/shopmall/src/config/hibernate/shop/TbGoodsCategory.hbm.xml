<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eh.shop.entity.TbGoodsCategory" table="TB_GOODS_CATEGORY">
		<id name="categoryId" type="java.lang.Long">
			<column name="CATEGORY_ID" precision="10" scale="0" />
			<generator class="native" />
		</id>
		<many-to-one name="parent" class="com.eh.shop.entity.TbGoodsCategory">
			<column name="PARENT_CATEGORY_ID" precision="10" scale="0" />
		</many-to-one>
		<property name="categoryName" type="java.lang.String">
			<column name="CATEGORY_NAME" length="300" />
		</property>
		<many-to-one name="shopInfo" class="com.eh.shop.entity.TbShopInfo">
			<column name="SHOP_ID" precision="10" scale="0" />
		</many-to-one>
		<property name="categoryUrl" type="java.lang.String">
			<column name="category_url" length="500" />
		</property>
		<property name="keyWords" type="java.lang.String">
			<column name="KEY_WORDS" length="500" />
		</property>
		<property name="categoryDesc" type="java.lang.String">
			<column name="CATEGORY_DESC" length="65535" />
		</property>
		<property name="dataStatus" type="java.lang.Long">
			<column name="DATA_STATUS" precision="10" scale="0" />
		</property>
		<property name="orderNum" type="java.lang.Long">
			<column name="ORDER_NUM" precision="10" scale="0" />
		</property>
		<property name="showAtNav" type="java.lang.Long">
			<column name="show_at_nav" length="2" />
		</property>
		<property name="treeNo" type="java.lang.String">
			<column name="TREE_NO" length="200" />
		</property>
		<property name="isLeaf" type="java.lang.Long">
			<column name="IS_LEAF" precision="10" scale="0" />
		</property>
		<property name="categoryLevel" type="java.lang.Long">
			<column name="CATEGORY_LEVEL" precision="10" scale="0" />
		</property>		
		<property name="jdUrl" type="java.lang.String">
			<column name="jd_url" length="500" />
		</property>
	</class>
</hibernate-mapping>