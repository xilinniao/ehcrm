<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.eh.shop.entity.TbOrderFlow" table="TB_ORDER_FLOW">    
		 <id name="flowId" type="java.lang.Long">
            <column name="FLOW_ID" precision="18" scale="0"/>
            <generator class="native" />
      	 </id>
		 <many-to-one name="order" class="com.eh.shop.entity.TbOrderMain">
			<column name="ORDER_ID" precision="10" scale="0"/>
		 </many-to-one>
		 <property name="flowNote" type="java.lang.String">
            <column name="FLOW_NOTE" length="300"/>
        </property>
        <property name="flowTime" type="java.util.Date">
			<column name="FLOW_TIME" length="7" />
		</property>
		<property name="userId" type="java.lang.Long">
            <column name="user_id" precision="18" scale="0"/>
        </property>
    </class>
</hibernate-mapping>