<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.eh.shop.entity.TbPageType" table="TB_PAGE_TYPE">    
		 <id name="typeId" type="java.lang.Long">
            <column name="TYPE_ID" precision="10" scale="0"/>
            <generator class="native" />
      	 </id>
		<many-to-one name="shopInfo"
			class="com.eh.shop.entity.TbShopInfo">
			<column name="SHOP_ID" precision="10" scale="0" />
		</many-to-one>
		 <property name="typeName" type="java.lang.String">
            <column name="TYPE_NAME" length="50"/>
        </property>
    </class>
</hibernate-mapping>