<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.eh.shop.entity.TbReceiptsSeq" table="tb_receipts_seq">
    
        <id name="recId" type="java.lang.Long">
            <column name="rec_id" precision="10" scale="0"/>
            <generator class="native" />
      	 </id>
				 
		<property name="seqType" type="java.lang.String">
            <column name="seq_type" length="50" />
        </property>
				 
       <property name="currentSeq" type="java.lang.Long">
            <column name="current_seq" precision="10" scale="0"/>
        </property>
				 
       <property name="todayNum" type="java.lang.String">
            <column name="today_num" length="6" />
        </property>
    </class>
</hibernate-mapping>