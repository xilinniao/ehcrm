<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eh.shop.entity.TbBrandInfo"
		table="TB_BRAND_INFO">
		<id name="brandId" type="java.lang.Long">
			<column name="brand_id" precision="10" scale="0" />
			<generator class="native" />
		</id>
		<many-to-one name="shopInfo"
			class="com.eh.shop.entity.TbShopInfo">
			<column name="SHOP_ID" precision="10" scale="0" />
		</many-to-one>
		<property name="introduction" type="java.lang.String">
			<column name="INTRODUCTION" length="65535" />
		</property>
		<property name="logo" type="java.lang.String">
			<column name="LOGO" length="255" />
		</property>
		<property name="name" type="java.lang.String">
			<column name="NAME" length="255" />
		</property>
		<property name="ordernum" type="java.lang.Long">
			<column name="ORDERNUM" precision="10" scale="0" />
		</property>
		<property name="url" type="java.lang.String">
			<column name="URL" length="255" />
		</property>
	</class>
</hibernate-mapping>