<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.eh.shop.entity.TbOrderDetail" table="TB_ORDER_DETAIL">    
		 <id name="detailId" type="java.lang.Long">
            <column name="DETAIL_ID" precision="10" scale="0"/>
            <generator class="native" />
      	 </id>		
         <many-to-one name="order" class="com.eh.shop.entity.TbOrderMain">
			<column name="ORDER_ID" precision="10" scale="0"/>
		 </many-to-one>
        <many-to-one name="goods" class="com.eh.shop.entity.TbGoodsInfo">
			<column name="GOODS_ID" precision="10" scale="0"/>
		 </many-to-one>
		 <property name="goodsPrice" type="java.lang.Double">
            <column name="GOODS_PRICE" precision="12" />
        </property>
		 <property name="goodsCount" type="java.lang.Long">
            <column name="GOODS_COUNT" precision="10" scale="0"/>
        </property>
		 <property name="goodsScore" type="java.lang.Long">
            <column name="GOODS_SCORE" precision="10" scale="0"/>
        </property>
    </class>
</hibernate-mapping>