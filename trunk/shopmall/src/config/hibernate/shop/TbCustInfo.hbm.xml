<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.eh.shop.entity.TbCustInfo" table="TB_CUST_INFO">    
		 <id name="custId" type="java.lang.Long">
            <column name="CUST_ID" precision="10" scale="0"/>
            <generator class="native" />
      	 </id>
		 <property name="custCode" type="java.lang.String">
            <column name="CUST_CODE" length="500"/>
        </property>
		 <property name="custPwd" type="java.lang.String">
            <column name="CUST_PWD" length="500"/>
        </property>
		 <property name="custName" type="java.lang.String">
            <column name="CUST_NAME" length="100"/>
        </property>
		 <property name="custLevel" type="java.lang.Long">
            <column name="CUST_LEVEL" precision="10" scale="0"/>
        </property>
		 <property name="custMobile" type="java.lang.String">
            <column name="CUST_MOBILE" length="50"/>
        </property>
		 <property name="custTel" type="java.lang.String">
            <column name="CUST_TEL" length="50"/>
        </property>
		 <property name="custAddr" type="java.lang.String">
            <column name="CUST_ADDR" length="200"/>
        </property>
		 <property name="custScore" type="java.lang.Long">
            <column name="CUST_SCORE" precision="10" scale="0"/>
        </property>
		 <property name="lastLoginTime" type="java.util.Date">
		 	<column name="LAST_LOGIN_TIME" length="10"/>
        </property>
		 <property name="createTime" type="java.util.Date">
		 	<column name="CREATE_TIME" length="10"/>
        </property>
    </class>
</hibernate-mapping>