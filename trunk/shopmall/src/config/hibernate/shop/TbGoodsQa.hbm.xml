<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eh.shop.entity.TbGoodsQa" table="tb_goods_qa">

		<id name="recId" type="java.lang.Long">
			<column name="rec_id" precision="10" scale="0" />
			<generator class="native" />
		</id>

		<many-to-one name="custInfo" class="com.eh.shop.entity.TbCustInfo">
			<column name="CUST_ID" precision="10" scale="0" />
		</many-to-one>
		<many-to-one name="shopInfo" class="com.eh.shop.entity.TbShopInfo">
			<column name="SHOP_ID" precision="10" scale="0" />
		</many-to-one>
		<many-to-one name="goods" class="com.eh.shop.entity.TbGoodsInfo">
			<column name="GOODS_ID" precision="10" scale="0" />
		</many-to-one>

		<property name="isPublish" type="java.lang.Long">
			<column name="is_publish" precision="1" scale="0" />
		</property>

		<property name="question" type="java.lang.String">
			<column name="question" length="300" />
		</property>

		<property name="answer" type="java.lang.String">
			<column name="answer" length="300" />
		</property>

		<property name="createTime" type="java.util.Date">
			<column name="create_time" length="7" />
		</property>

		<property name="answerTime" type="java.util.Date">
			<column name="answer_time" length="7" />
		</property>
	</class>
</hibernate-mapping>