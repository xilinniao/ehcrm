<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.eh.shop.entity.TbGoodsInfoSub" table="TB_GOODS_INFO_SUB">    
		<id name="goodsSubId" type="java.lang.Long">
            <column name="GOODS_SUB_ID" precision="10" scale="0"/>
            <generator class="native" />
      	</id>
        <many-to-one name="goods" class="com.eh.shop.entity.TbGoodsInfo">
			<column name="GOODS_ID" precision="10" scale="0"/>
		</many-to-one>
		 <property name="goodsSubName" type="java.lang.String">
            <column name="GOODS_SUB_NAME" length="200"/>
        </property>
		 <property name="goodsSubNo" type="java.lang.String">
            <column name="GOODS_SUB_NO" length="50"/>
        </property>
		 <property name="marketPrice" type="java.lang.Double">
            <column name="MARKET_PRICE" precision="12" scale="2"/>
        </property>
		 <property name="discountPrice" type="java.lang.Double">
            <column name="DISCOUNT_PRICE" precision="12" scale="2"/>
        </property>
		 <property name="leavesCount" type="java.lang.Long">
            <column name="LEAVES_COUNT" precision="10" scale="0"/>
        </property>
		<property name="dataStatus" type="java.lang.Long">
            <column name="DATA_STATUS" precision="10" scale="0"/>
        </property>
    </class>
</hibernate-mapping>