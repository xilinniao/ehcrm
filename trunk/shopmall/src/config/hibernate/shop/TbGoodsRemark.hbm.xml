<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.eh.shop.entity.TbGoodsRemark" table="tb_goods_remark">
    
        <id name="recId" type="java.lang.Long">
            <column name="rec_id" precision="10" scale="0"/>
            <generator class="native" />
      	 </id>
				 
       <property name="custId" type="java.lang.Long">
            <column name="cust_id" precision="10" scale="0"/>
        </property>
				 
		<property name="custName" type="java.lang.String">
            <column name="cust_name" length="50" />
        </property>
				 
       <property name="goodsId" type="java.lang.Long">
            <column name="goods_id" precision="10" scale="0"/>
        </property>
				 
		<property name="content" type="java.lang.String">
            <column name="content" length="500" />
        </property>
				 
       <property name="descSocre" type="java.lang.Long">
            <column name="desc_socre" precision="10" scale="0"/>
        </property>
				 
       <property name="serviceScore" type="java.lang.Long">
            <column name="service_score" precision="10" scale="0"/>
        </property>
				 
       <property name="speedScore" type="java.lang.Long">
            <column name="speed_score" precision="10" scale="0"/>
        </property>
				 
		<property name="createTime" type="java.util.Date">            
            <column name="create_time" length="7"/>
        </property>
    </class>
</hibernate-mapping>