<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.eh.shop.entity.TbTuanInfo" table="TB_TUAN_INFO">
    
        <id name="tuanId" type="java.lang.Long">
            <column name="tuan_id" precision="10" scale="0"/>
            <generator class="native" />
      	 </id>
				 
       <property name="applyId" type="java.lang.Long">
            <column name="apply_id" precision="10" scale="0"/>
        </property>
				 
       <many-to-one name="shopInfo"
			class="com.eh.shop.entity.TbShopInfo">
			<column name="SHOP_ID" precision="10" scale="0" />
		</many-to-one>
				 
       <property name="isPublish" type="java.lang.Long">
            <column name="is_publish" precision="2" scale="0"/>
        </property>
				 
		<property name="linkTel" type="java.lang.String">
            <column name="link_tel" length="50" />
        </property>
				 
		<property name="linkQq" type="java.lang.String">
            <column name="link_qq" length="100" />
        </property>
				 
       <many-to-one name="faceImage" class="com.eh.base.entity.TbAttachment">
			<column name="FACE_IMAGE_ID" precision="12" scale="0" />
		</many-to-one>
				 
		<property name="topicName" type="java.lang.String">
            <column name="topic_name" length="200" />
        </property>
				 
		<property name="beginDate" type="java.util.Date">            
            <column name="begin_date" length="7"/>
        </property>
				 
		<property name="endDate" type="java.util.Date">            
            <column name="end_date" length="7"/>
        </property>
				 
        <property name="price" type="java.lang.Double">
            <column name="price" precision="12" />
        </property>
				 
        <property name="discountPrice" type="java.lang.Double">
            <column name="discount_price" precision="12" />
        </property>
				 
       <property name="maxQty" type="java.lang.Long">
            <column name="max_qty" precision="10" scale="0"/>
        </property>
				 
       <property name="currQty" type="java.lang.Long">
            <column name="curr_qty" precision="10" scale="0"/>
        </property>
				 
       <property name="createUser" type="java.lang.Long">
            <column name="create_user" precision="10" scale="0"/>
        </property>
				 
		<property name="createTime" type="java.util.Date">            
            <column name="create_time" length="7"/>
        </property>
        
        <one-to-one name="ext" class="com.eh.shop.entity.TbTuanInfoExt" cascade="all"></one-to-one>
    </class>
</hibernate-mapping>