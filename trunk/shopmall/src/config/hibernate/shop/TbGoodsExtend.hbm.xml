<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.eh.shop.entity.TbGoodsExtend" table="TB_GOODS_EXTEND">    
		 <id name="goodsId" type="java.lang.Long">
            <column name="GOODS_ID" precision="10" scale="0"/>
            <generator class="foreign">
				<param name="property">goods</param>
			</generator>
      	 </id>
      	 <one-to-one name="goods" class="com.eh.shop.entity.TbGoodsInfo" cascade="save-update"></one-to-one>
		 <property name="goodsDesc" type="java.lang.String">
            <column name="GOODS_DESC" length="65535"/>
        </property>
		 <property name="packList" type="java.lang.String">
            <column name="PACK_LIST" length="1000"/>
        </property>
    </class>
</hibernate-mapping>