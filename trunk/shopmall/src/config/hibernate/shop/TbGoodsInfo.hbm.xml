<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eh.shop.entity.TbGoodsInfo"
		table="TB_GOODS_INFO" lazy="false">
		<id name="goodsId" type="java.lang.Long">
			<column name="GOODS_ID" precision="11" scale="0" />
			<generator class="native" />
		</id>
		<many-to-one name="shopInfo"
			class="com.eh.shop.entity.TbShopInfo">
			<column name="SHOP_ID" precision="10" scale="0" />
		</many-to-one>
		<many-to-one name="category"
			class="com.eh.shop.entity.TbGoodsCategory">
			<column name="category_id" precision="10" scale="0" />
		</many-to-one>
		<many-to-one name="siteCategory"
			class="com.eh.shop.entity.TbSiteCategory">
			<column name="site_category" precision="12" scale="0" />
		</many-to-one>
		<many-to-one name="brandInfo"
			class="com.eh.shop.entity.TbBrandInfo">
			<column name="brand_id" precision="10" scale="0" />
		</many-to-one>
		<many-to-one name="indexCategory"
			class="com.eh.shop.entity.TbIndexCategory">
			<column name="page_category_id" precision="10" scale="0" />
		</many-to-one>
		<property name="goodsName" type="java.lang.String">
			<column name="GOODS_NAME" length="200" />
		</property>
		<property name="keyWord" type="java.lang.String">
			<column name="KEY_WORD" length="500" />
		</property>
		<property name="goodNo" type="java.lang.String">
			<column name="GOOD_NO" length="100" />
		</property>		
		<property name="weight" type="java.lang.Double">
			<column name="weight" precision="12" />
		</property>
		<property name="marketPrice" type="java.lang.Double">
			<column name="market_price" precision="12" />
		</property>
		<property name="discountPrice" type="java.lang.Double">
			<column name="DISCOUNT_PRICE" precision="12" />
		</property>
		<property name="leavesCount" type="java.lang.Long">
			<column name="LEAVES_COUNT" precision="10" scale="0" />
		</property>		
		<property name="isRecommend" type="java.lang.Long">
			<column name="IS_RECOMMEND" precision="1" scale="0" />
		</property>
		<many-to-one name="faceImage" class="com.eh.base.entity.TbAttachment">
			<column name="FACE_IMAGE_ID" precision="12" scale="0" />
		</many-to-one>
		<property name="jdUrl" type="java.lang.String">
			<column name="jd_url" length="500" />
		</property>
		<property name="defaultGoodsSub" type="java.lang.Long">
			<column name="DEFAULT_GOODS_SUB" precision="11" scale="0" />
		</property>		
		<property name="orderNum" type="java.lang.Long">
			<column name="order_num" precision="5" scale="0" />
		</property>
		<property name="createUser" type="java.lang.Long">
			<column name="CREATE_USER" precision="12" scale="0" />
		</property>
		 <property name="createTime" type="java.util.Date">
		 	<column name="CREATE_TIME" length="7" />
        </property>
        <one-to-one name="ext" class="com.eh.shop.entity.TbGoodsExtend" cascade="save-update"></one-to-one>
	</class>
</hibernate-mapping>