<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eh.shop.entity.TbPageGoodsRel"
		table="TB_PAGE_GOODS_REL">
		<id name="recId" type="java.lang.Long">
			<column name="REC_ID" precision="10" scale="0" />
			<generator class="native" />
		</id>
		<many-to-one name="goodsInfo"
			class="com.eh.shop.entity.TbGoodsInfo">
			<column name="GOODS_ID" precision="10" scale="0" />
		</many-to-one>
		<many-to-one name="pageCategory"
			class="com.eh.shop.entity.TbPageCategory">
			<column name="CATEGORY_ID" precision="10" scale="0" />
		</many-to-one>
		<property name="orderNum" type="java.lang.Long">
			<column name="ORDER_NUM" precision="10" scale="0" />
		</property>
		<property name="createUser" type="java.lang.Long">
			<column name="CREATE_USER" precision="12" scale="0" />
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="CREATE_TIME" length="7" />
		</property>
	</class>
</hibernate-mapping>