<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eh.shop.entity.TbTuanInfoExt"
		table="TB_TUAN_INFO_EXT">

		<id name="extId" type="java.lang.Long">
			<column name="tuan_id" precision="10" scale="0" />
			<generator class="foreign">
				<param name="property">apply</param>
			</generator>
		</id>
		
		<one-to-one name="info" class="com.eh.shop.entity.TbTuanInfo"></one-to-one>

		<property name="rules" type="java.lang.String">
			<column name="rules" length="65535" />
		</property>

		<property name="topicContent" type="java.lang.String">
			<column name="topic_content" length="65535" />
		</property>
	</class>
</hibernate-mapping>